Sprite.prototype.isVisible = function() {
   return (this.layer.currentY + this.y &lt; viewHeight)
      &amp;&amp; (this.layer.currentY + this.y + this.states[this.state].bounds.height &gt;= 0)
      &amp;&amp; (this.layer.currentX + this.x &lt; viewWidth)
      &amp;&amp; (this.layer.currentX + this.x + this.states[this.state].bounds.width &gt;= 0);
};</Text>
